{
  "$schema": ".markprint/schemas/pipeline-profile.schema.json",
  "extends": null,
  "profile": {
    "id": "standard-letter",
    "label": "Standard Letter",
    "version": "1.0.0",
    "description": "Standard letter format based on DTS Master Report layout profile",
    "category": "LETTER",
    "schema": ".markprint/schemas/standard-letter.schema.json",
    "renderer": {
      "engine": "chromium",
      "options": {}
    },
    "outputs": {
      "pdf": {
        "enabled": true,
        "mode": "ACTIVE_ONLY",
        "filename_pattern": "{document_id}_r{revision}.pdf",
        "target_directory": "published/pdf"
      },
      "html": {
        "enabled": true,
        "mode": "DISABLED",
        "filename_pattern": "{document_id}_r{revision}.html",
        "target_directory": "published/web"
      },
      "png": {
        "enabled": false
      },
      "jpeg": {
        "enabled": false
      }
    },
    "buildMode": "manual"
  },
  "layout": {
    "type": "json",
    "source": "${extensionPath}/templates/layouts/standard-letter.layout.json",
    "rendererHint": "chromium",
    "label": "Standard Letter Layout"
  },
  "resources": {
    "css": [],
    "fonts": [
      {
        "family": "Arial",
        "style": "Regular",
        "weight": 400
      },
      {
        "family": "Trebuchet MS",
        "style": "Regular",
        "weight": 400
      }
    ],
    "assets": []
  },
  "artifactMappings": {}
}
